<?xml version="1.0" encoding="utf-8"?>
<Application xmlns='http://schemas.microsoft.com/windows/2009/Ribbon'>
  <Application.Commands>
    <Command Name="cmdButtonSigma"
             Symbol="ID_CMD_SIGMA"
             LabelTitle="sigma">
      <Command.LargeImages>
        <Image>Res\sigma.bmp</Image>
      </Command.LargeImages>
    </Command>
    <Command Name="cmdButtonFraction"
             Symbol="ID_CMD_FRACTION"
             LabelTitle="fraction">
      <Command.LargeImages>
        <Image>Res\fraction.bmp</Image>
      </Command.LargeImages>
    </Command>
  </Application.Commands>
  <Application.Views>
    <Ribbon>
      <Ribbon.Tabs>
        <Tab CommandName="cmdTabSymbols">
          <Group CommandName="cmdGroupSetOperations" SizeDefinition="OneButton">
            <Button CommandName="cmdButtonSigma" />
          </Group>
          <Group CommandName="cmdGroupNumberOperations" SizeDefinition="OneButton">
            <Button CommandName="cmdButtonFraction" />
          </Group>
        </Tab>
      </Ribbon.Tabs>
    </Ribbon>
  </Application.Views>
</Application>